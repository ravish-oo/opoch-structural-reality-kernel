    <!-- Agent 1 goal (0,11) = (0.25, 5.75) - Teal -->
    <model name="goal_1"><static>true</static><pose>0.25 5.75 0.003 0 0 0</pose><link name="link"><visual name="v"><geometry><cylinder><radius>0.15</radius><length>0.002</length></cylinder></geometry><material><ambient>0.31 0.80 0.77 0.5</ambient></material></visual></link></model>

