    <model name="grid_line_v5"><static>true</static><pose>2.5 3 0.002 0 0 0</pose><link name="link"><visual name="v"><geometry><box><size>0.01 6 0.001</size></box></geometry><material><ambient>0.7 0.7 0.7 1</ambient></material></visual></link></model>
