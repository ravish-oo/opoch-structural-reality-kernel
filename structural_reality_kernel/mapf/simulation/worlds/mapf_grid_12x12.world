      <static>true</static>
      <pose>3 6.15 0.125 0 0 0</pose>
      <link name="link">
        <collision name="collision"><geometry><box><size>6.3 0.1 0.25</size></box></geometry></collision>
        <visual name="visual"><geometry><box><size>6.3 0.1 0.25</size></box></geometry><material><ambient>0.3 0.3 0.3 1</ambient></material></visual>
      </link>
    </model>

